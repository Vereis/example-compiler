// Basic testing of switch statement

let res : int = test(1)

function test (x : int) : int {
    input x
    switch x
        // 1
        case 1 { 
            x := 2
            x := 3
        }
        // 32
        case a(0) { 
            x := 32
        }
        // 32
        case 32 {
            x := 64
        }
        // 0
        case 0 {

        }
        // default
        default { 
            x := 5
        }

    output x
    return x
}

function a (x : int) : int {
    x := x + 32
    return x
}