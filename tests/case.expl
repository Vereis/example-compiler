// Basic testing of switch statement

let res : int = test(1)

function test (x : int) : int {
    input x
    switch x
        case 0 + 1 { 
            x := 2
            x := 3
        }
        case 2 * 16 { 
            x := 4
        }
        default { 
            x := 5
        }

    output x
    return x
}
